</table>

		<div id="hts-container" class="hts-container">
			<svg id="hts-svgcontainer" class="hts-container" width="1150"
				height="150">
			</svg>
		</div>
	<div>{{ form_rest(form) }}</div>	
	<div>{{ form_widget(form.submit) }}</div>
</form>