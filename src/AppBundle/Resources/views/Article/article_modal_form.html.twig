<form id="article_modal">
	{{ form_errors(form) }}
	<table class="table-condensed">
		<tr>
			<td />
			{{ form_label(form.title) }}
			<td />
			{{ form_widget(form.title) }}
		</tr>
		<tr>
			<td />
			{{ form_label(form.type) }}
			<td />
			{{ form_widget(form.type) }}
		</tr>
		<tr>
			<td />
			{{ form_label(form.subType) }}
			<td />
			{{ form_widget(form.subType) }}
		</tr>
		<tr>
			<td />
			{{ form_label(form.abstract) }}
			<td />
			{{ form_widget(form.abstract) }}
		</tr>
		<tr id="article_modal_isBeginDateApprox_row">
			<td />
			{{ form_label(form.isBeginDateApprox) }}
			<td />
			{{ form_widget(form.isBeginDateApprox) }}
		</tr>
		<tr id="article_modal_beginDate_row">
			<td />
			{{ form_label(form.beginDate) }}
			<td />
			{{ form_widget(form.beginDate) }}
		</tr>
		<tr id="article_modal_minBeginDate_row">
			<td />
			{{ form_label(form.minBeginDate) }}
			<td />
			{{ form_widget(form.minBeginDate) }}
		</tr>
		<tr id="article_modal_maxBeginDate_row">
			<td />
			{{ form_label(form.maxBeginDate) }}
			<td />
			{{ form_widget(form.maxBeginDate) }}
		</tr>
		<tr id="article_modal_hasNotEndDate_row">
			<td />
			{{ form_label(form.hasNotEndDate) }}
			<td />
			{{ form_widget(form.hasNotEndDate) }}
		</tr>
		<tr id="article_modal_isEndDateApprox_row">
			<td />
			{{ form_label(form.isEndDateApprox) }}
			<td />
			{{ form_widget(form.isEndDateApprox) }}
		</tr>
		<tr id="article_modal_endDate_row">
			<td />
			{{ form_label(form.endDate) }}
			<td />
			{{ form_widget(form.endDate) }}
		</tr>
		<tr id="article_modal_minEndDate_row">
			<td />
			{{ form_label(form.minEndDate) }}
			<td />
			{{ form_widget(form.minEndDate) }}
		</tr>
		<tr id="article_modal_maxEndDate_row">
			<td />
			{{ form_label(form.maxEndDate) }}
			<td />
			{{ form_widget(form.maxEndDate) }}
		</tr>
	</table>
	<div hidden=true><input hidden=true type="submit" id="article_modal_fake_submitter" class="validateDontSubmit" value=""></div>
</form>