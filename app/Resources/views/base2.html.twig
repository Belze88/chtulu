<!DOCTYPE html>
<html lang="en">
<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{% block title %}HistoricaBase | Blank Page 2{% endblock %}</title>
        <meta
                content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
                name="viewport">
        <link rel="stylesheet" href="{{ asset('build/app.css') }}" />




        {% block stylesheets %}
        {% endblock %}
    {% endblock %}
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div id="hb-wrapper" class="hold-transition skin-blue sidebar-mini"></div>
</body>
<div id="hb-data" hidden>
    {% block data %}
        <div data-url="{{ url('crud_get') }}" id="hb-url-crud-get"></div>
        <div data-url="{{ url('crud_get_one_by_id') }}" id="hb-url-crud-get-one-by-id"></div>
        <div data-url="{{ url('crud_get_new') }}" id="hb-url-crud-get-new"></div>
        <div data-url="{{ url('crud_post') }}" id="hb-url-crud-post"></div>
        {% for dto in dtoList() %}
            <div class="hb-dto-mapping" data-mapping="{{ dto|getMapping() }}" id="hb-mapping-{{ dto|getAbridgedName() }}"></div>
        {% endfor %}
        <div data-token="{{ csrf_token("token_id") }}" id="hb-security-token"></div>
    {% endblock %}
</div>
<script src="{{ asset('build/app.js') }}"></script>
<script src="{{ asset('build/layout-react.js') }}"></script>
{% block javascript %}
{% endblock %}
</html>